@using ProcessManager.Models
@model BaoCunBiaoDan
@{
    ViewBag.Title = "BaoCunBiaoDan";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Styles.Render("~/Content/css/neirong");

<div class="row">
    <div>
        @{
            foreach (BaoCunBiaoDan item in Model.lbcb)
            {
                <div class="media">
                    <div class="media-left">
                    </div>

                    <div class="media-body">
                        <h4 class="media-heading"><a href="@item.url/@item.bid">@item.leixing:@item.bid</a></h4>
                    </div>
                </div>
            }

        }
    </div>
</div>

