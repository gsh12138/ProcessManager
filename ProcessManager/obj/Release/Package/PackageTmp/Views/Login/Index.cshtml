
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>登录</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/css/login")
</head>
<body>
    <div class="container-fluid">
        <div class="row">

            <div class="col-md-4 col-md-offset-8">
                @Html.Partial("LoginBox")
            </div>

        </div>
    </div>

    <footer>
        <p>&copy; @DateTime.Now.Year - Prowerby shsng</p>
        
    </footer>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @{
        if (ViewBag.modelerr != null)
        {
            foreach (ProcessManager.Controllers.ModelErr me in ViewBag.modelerr)
            {
                <script>
                    $('#@me.ziduan').addClass('has-error');
                </script>
            }
        }
    }
    
</body>
</html>
